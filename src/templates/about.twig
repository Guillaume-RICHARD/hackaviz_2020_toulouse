{% block content %}

    <div id="page-wrapper">
        <!-- Wrapper -->
        <section id="wrapper">

            <div class="wrapper">
                <div class="inner">
                    <h2 class="major">A propos</h2>

                    <section>
                        <h3 class="major">... de moi</h3>

                        <blockquote>
                            <div class="card-body">
                                <h5 class="card-title">Nom : Guillaume RICHARD</h5>
                                <h6 class="card-subtitle mb-2 text-muted">Métier : Développeur Web Back-end</h6>
